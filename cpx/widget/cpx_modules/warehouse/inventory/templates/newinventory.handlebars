<span id="mdError" class="mdError"></span>
<form id="createInventory" action="/web/expense/createinventory" method="post">
<div class="cpx-form">
	<div class="form-group control-group">
	    <label class="control-label">盘点名称</label>
	    <div class="controls">
	        <div class="control">
	            <input type="text" class="form-control" id="inventoryName" name="inventoryName">
	        </div>
	    </div>
	</div>
	<div class="form-group control-group">
	    <label class="control-label">盘点位置</label>
	    <div class="controls">
	        <div class="control">
	        	<input type="hidden" value="" id="inventoryPositionVal" name="inventoryPositionVal"/>
	            <div id="inventoryPositionDiv" class="select-control">
	                <input type="hidden" value="" id="inventoryPosition" name="inventoryPosition"/>
	                <span class="select-txt fn-text-overflow"></span>
	                <i class="select-icon glyphicon glyphicon-chevron-down"></i>
	                <ul id="inventoryPositon">

	                </ul>
	            </div>
	        </div>
	    </div>
	</div>
</div>
</form>